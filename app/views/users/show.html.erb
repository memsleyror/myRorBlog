<p>Find me in app/views/users/show.html.erb</p>

<h1>Users#show View</h1>
<h1>@user.last_name:<%= @user.last_name %></h1>
<h2>@user.first_name:<%= @user.first_name %></h2>


<h3>Posts</h3>
<div class="posts">
	<% @user.posts.each do |post| %>
		<div class "movie">
			<span><%= link_to post.title, post %></span>
			(<%= post.body %>)
		</div>
	<% end %>
</div>
